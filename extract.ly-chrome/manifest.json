{
    "name": "Note taking with Extract.ly",
    "description": "Take note ",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "tabs",
      "*://*/*",
      "clipboardRead",
      "storage",
      "contextMenus"
    ],
    "icons": {"16": "img/filter.png"},
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_title": "extract.ly",
      "default_popup": "popup.html",
      "default_icon" : "img/filter.png"
    },
    "manifest_version": 2,
    "options_page": "options.html"
  }
  